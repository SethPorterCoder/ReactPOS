CREATE TABLE users (
	userID int not null,
	userPassword int not null,
	fName varchar(10),
	lName varchar(10),
	unique (userID)
);

insert into users values (5491, 94, "Seth", "Porter");

create table items (
	itemID INT AUTO_INCREMENT PRIMARY KEY,
	name varchar(25),
	price decimal(10, 2)
);

insert into items values(NULL, 'Cupcake', 25.99);